<tr>
  <td><%= ticket.number %></td>
  <td><%= "#{ticket.first_station.title} - #{ticket.last_station.title}" %></td>
  <td><%= "#{ticket.last_name} #{ticket.first_name}" %></td>
  <td><%= link_to 'Show', admin_ticket_path(ticket) %></td>
  <td><%= link_to 'Destroy', [:admin, ticket], method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
